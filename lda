Sub RepeatRows()
    Dim ws As Worksheet
    Set ws = ThisWorkbook.Sheets(1) 'Assuming your data is in the first sheet
    
    Dim lastRow As Long
    lastRow = ws.Cells(ws.Rows.Count, "A").End(xlUp).Row 'Find last row in the data

    Dim repeatCount As Integer
    repeatCount = 10 'Number of times to repeat

    Dim i As Long, j As Long
    For i = lastRow To 1 Step -1
        For j = 1 To repeatCount - 1
            ws.Rows(i).Copy
            ws.Rows(i + 1).Insert Shift:=xlDown
        Next j
    Next i
End Sub
